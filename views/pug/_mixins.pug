- function getImageSize(name) { return name.match('[0-9]+(x)[0-9]+')[0].split('x'); }


//- Img image
mixin img(name, className)
  - var size = getImageSize(name);
  img(src='images/' + name, alt='', width=size[0], height=size[1], class=className)


//- Jpg image
mixin jpg(name, className)
  - var size = getImageSize(name);
  img(src='images/' + name + '.jpg', alt='', width=size[0], height=size[1], class=className)


//- Png image
mixin png(name, className)
  - var size = getImageSize(name);
  img(src='images/' + name + '.png', alt='', width=size[0], height=size[1], class=className)

mixin loop(count)
  - for (var i = 0; i < count; i++)
    block

//- Parallax Section
mixin parallax(obj)
  section.section.parallax-container(data-parallax-img= obj.img)
    .parallax-content(class= obj.addClass)
      block

//- video - Video object
mixin bg-vide( video )
  .bg-vide( data-vide-bg= video.src, data-vide-options= video.options )
    block

mixin thumbnail-small(obj)
  a.thumbnail-small(href=obj.link)
    .thumbnail-small-image
      img(src='images/layout-panel/' + obj.filename + '.jpg', alt='', width='380', height='280')
    .thumbnail-small-hover-image
      img(src='images/layout-panel/' + obj.filename + '.jpg', alt='', width='380', height='280')

    .thumbnail-small-caption
      p.thumbnail-small-header=obj.title

//- Blockquote default
//- {quoteClass, quote, imgName, imgClassName, author, department, position}
mixin blockquote-default(data)
  blockquote.quote.quote-default(class=data.quoteClass)
    .quote-icon.mdi.mdi-format-quote
    .quote-body
      q=data.quote
    if data.authorImg
      +jpg(data.authorImg, "quote-img")
    .author: cite=data.authorName
    if data.authorPosition
      .position=data.authorPosition

//- Blockquote custom
//- {quoteClass, quote, imgName, imgClassName, author, department, position}
mixin blockquote-custom(data)
  blockquote.quote.quote-boxed(class=data.quoteClass)
    .quote-cloud
      .quote-icon.mdi.mdi-format-quote
      .quote-body
        q=data.quote
    .quote-meta.unit.unit-horizontal.unit-middle
      .unit-left
        +jpg(data.authorImg, "quote-img")
      .unit-body
        .author: cite=data.authorName
        .position=data.authorPosition

//- Blurb
//- {iconClass, iconClass, imgName, title, titleLink, content}
mixin blurb(data)
  .blurb(class=data.blurbClass)
    .icon(class=data.iconClass)
    h5.blurb-title: a(href=data.titleLink) #{data.title}
    p.blurb-content #{data.content}

//- Blurb left
//- {iconClass, iconClass, imgName, title, titleLink, content}
mixin blurb-left(data)
  .blurb-left.unit.unit-spacing-sm.unit-md-horizontal(class=data.blurbClass)
    .unit-left
      .icon.icon-primary(class=data.iconClass)
    .unit-body
      h4.blurb-title=data.title
      .divider.divider-1
      p.blurb-content=data.content

//- Blurb boxed
//- {iconClass, title, content}
mixin blurb-boxed(data)
  .blurb-boxed
