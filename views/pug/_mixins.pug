//- Photo Swipe item
mixin psw-item(image, imageOriginal)
  a.thumbnail-gallery(data-photo-swipe-item="", data-size=imageOriginal.split('-').pop(), href='images/' + imageOriginal + '.jpg')
    .thumbnail-overlay
      +jpg(image)

//- Post modern
//- {imageName, title, link, exeption, date, tag.title, tag.link, comments, commentsLink, author, authorLink}
mixin post-modern(data, meta, tags)
  article.post-modern
    .post-left
      .post-meta-wrap
        .post-date
          .day=meta.day
          .month=meta.month
      .post-author
        span by
        =" "
        a(href=meta.authorLink)=meta.author

    .post-body
      .media-wrapper
        a(href=data.link)
          +jpg(data.imageName)
      .content
        h4.post-title: a.heading-link(href=data.link)=data.title
        p.post-exeption=data.exeption
        .post-meta
          ul.list-meta
            li
              each tag, i in tags
                a(href= tag.href).link= tag.title
                - if (i !== (tags.length - 1))
                  = ", "
            li
              a.comments(href=meta.commentsLink)=meta.comments
              =" "
              span Comments

//- Video box
//- {background, link}
mixin video-box(data)
  a.mfp-link.video-cover(style="background-image: url('images/" + data.background + "');" data-lightbox="iframe" href=data.link)
    span.icon.mdi.mdi-play-circle-outline

//- Post classic
//- {imageName, title, link, exeption}
mixin post-classic(data, tags)
  article.post-classic(class=data.postClass)
    .media-wrapper
      a(href=data.link)
        +jpg(data.imageName)
    ul.group.group-sm.post-meta-inline
      li.list-item
        .icon.mdi.mdi-tag
        each tag, i in tags
          a(href= tag.href).post-tag= tag.title
          - if (i !== (tags.length - 1))
            = ", "
      li.list-item
        .icon.mdi.mdi-comment
        a(href=data.commentLink)=data.comment
      li.list-item
        .icon.mdi.mdi-account
        a(href=data.authorLink)=data.author

    .content
      h4.post-title: a(href=data.link)=data.title
      .post-date=data.date
      p.post-exeption=data.exeption

//- Post minimal
//- {imageName, title, link, exeption}
mixin post-minimal(data)
  .post-minimal(style="background-image: url(images/" + data.bgImage + '.jpg);')
    .post-inner
      .post-tags
        a(href=data.tagLink).tag-2=data.tag
      .post-content
        .post-date=data.time
        h4.title: a(href=data.link)=data.title

//- Post boxed
//- {imageName, title, titleLink, date, exeption, infoCountN, infoTitleN}
mixin post-boxed(data)
  .post-boxed
    .media
      +jpg(data.imgName)
    .caption
      .post-date=data.time
      h4.title: a(href=data.titleLink)=data.title
      .exeption=data.exeption
    ul.info
      li
        .icon.mdi.mdi-tag
        a(href=data.tagLink)=data.tag
      li
        .icon.mdi.mdi-comment
        a(href=data.commentLink)=data.comment
      li
        .icon.mdi.mdi-account
        a(href=data.authorLink)=data.author

//- Comments
//- {commentClass, imgName, authorName, link, date, time, replyAuthor, comment}
mixin comment(data)
  .box-comment
    .box-comment-image
      +jpg(data.imgName)
    .box-comment-body
      ul.list-inline-3
        li
          .title: a(href=data.link) #{data.authorName}
        li.comment-meta
          span #{data.date}
        if (data.replyAuthor)
          li.comment-meta
            .reply-author
              .icon.mdi.mdi-reply
              span=data.replyAuthor
      p.comment #{data.comment}

//- Gallery item
//- {imageName, imageOriginal, title, exeption}
mixin gallery-item(data)
  .gallery-item(class=data.itemClass)
    a.gallery-item-inner(data-lightgallery="item", href='images/originals/' + data.imageOriginal + '.jpg')
      .gallery-img-wrap
        +jpg(data.imageName)
      .zoom-item
    .caption
      h4.title
        a(href=data.titleLink)=data.title

//- Aside gallery item
//- {imageName, imageOriginal}
mixin aside-gallery-item(data)
  a.aside-gallery-item(data-lightgallery="item" href='images/originals/' + data.imageOriginal + '.jpg')
    +jpg(data.imageName)
    span.mdi.mdi-plus

//- Aside gallery item
//- {imageName, imageOriginal}
mixin gallery-item-wide(data)
  a.gallery-item-wide(data-lightgallery="item" href='images/originals/' + data.imageOriginal + '.jpg')
    +jpg(data.imageName)
    span.icon.mdi.mdi-loupe

//- Flickr gallery item
//- {imageName, imageOriginal}
mixin flickr-item(data)
  a.flickr-item(data-lightgallery="item" data-type="flickr-item", data-image_b="href", data-photo-swipe-item)
    img(src='images/_blank.png', data-image_n="src" alt="")
    span.mdi.mdi-plus

//- Contact box
//- {location, phone, email}
mixin contact-box(data, lists)
  .contact-box
    if (data.location)
      .contact-item
        .contact-icon
          .icon.icon-xs.mdi.mdi-map-marker
        .contact-body
          p: a(href=data.locationLink)=data.location
    if (lists)
      .contact-item
        .contact-icon
          .icon.icon-xs.mdi.mdi-phone
        .contact-body
          each list, i in lists
            a(href="tel:" + list.phoneLink)= list.phone
            - if (i !== (lists.length - 1))
              = ", "
    if (data.email)
      .contact-item
        .contact-icon
          .icon.icon-xs.mdi.mdi-email
        .contact-body
          p: a(href="mailto:" + data.emailLink)=data.email

//- Location
//- {imageName, title, titleLink, location, phone, infoCountN, infoTitleN}
mixin location(data, lists)
  .thumbnail-1
    .media
      +jpg(data.imgName)
    .caption
      h4.title: a(href=data.titleLink)=data.title
      .contact-box
        .contact-item
          .contact-icon
            .icon.icon-xs.mdi.mdi-map-marker
          .contact-body
            p: a(href=data.locationLink)=data.location
        .contact-item
          .contact-icon
            .icon.icon-xs.mdi.mdi-phone
          .contact-body
            p: a(href="tel:" + data.phoneLink)=data.phone
    ul.info
      each list, i in lists
        li
          .info-count=list.infoCount
          .info-title=list.infoTitle

