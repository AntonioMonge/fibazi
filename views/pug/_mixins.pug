mixin deal(data, lists)
  .thumbnail-3
    .media-wrapper
      a(href=data.link)
        +jpg(data.imageName)
    .content
      .place=data.place
      h4.title: a(href=data.link)=data.title
      ul.info
        each list, i in lists
          li: p= list.opportunity
      p.exeption=data.exeption
